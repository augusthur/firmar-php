{
    "name": "augusthur/firmar-php",
    "type": "library",
    "description": "PHP integration with https://firmar.gob.ar platform",
    "keywords": [
        "digital signature",
        "firmar.gob.ar",
        "argentina"
    ],
    "homepage": "https://github.com/augusthur/firmar-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Augusto Mathurin",
            "homepage": "https://github.com/augusthur"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Augusthur\\FirmarIntegration\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Augusthur\\FirmarIntegration\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
